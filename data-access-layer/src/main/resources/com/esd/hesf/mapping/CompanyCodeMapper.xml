<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.esd.hesf.dao.CompanyCodeDao">

	<!-- update -->
	<update id="update" parameterType="java.lang.Integer">
		update company_code set id =  id + 1;
	</update>
	
	<!-- retrieve -->
	<select id="retrieveCompanyCode" parameterType="java.lang.Integer" resultType="java.lang.Integer">
		select id from company_code;
	</select>
	
</mapper>